{
  "extends": "tslint:recommended",
  "rules": {
    "object-literal-sort-keys": false,
    "quotemark": false,
    "trailing-comma": false,
    "whitespace": false,
    "member-ordering": false,
    "member-access": false,
    "typedef-whitespace": false,
    "interface-name": false,
    "max-line-length": [true, 150],
    "no-console": false
  }
}
